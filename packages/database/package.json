{
  "name": "@maskwise/database",
  "version": "1.0.0",
  "description": "Database schemas and migrations for Maskwise platform",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "migrate": "npx prisma migrate dev",
    "generate": "npx prisma generate",
    "studio": "npx prisma studio"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/pdfkit": "^0.17.2",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "pdfkit": "^0.17.1",
    "prisma": "^5.6.0",
    "uuid": "^9.0.1"
  },
  "prisma": {
    "seed": "npx ts-node src/seed.ts"
  }
}
